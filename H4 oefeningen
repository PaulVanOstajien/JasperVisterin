hold on 
fimplicit3(parabool)
xlim([-7,7])
ylim([-7,7])
zlim([-7,7])

syms a
paraboloide = y^2+z^2-4*x*a
parabool=y^2-x*a
vlak= x-3*a
syms theta u t

parparam=[x,u*cos(t),u*sin(t)]
snijlijn_1=solve(paraboloide==parabool,x)
snijlijn_2=solve(vlak==paraboloide,x)

snijlijn=subs(snijlijn_1,[x,y,z],parparam)
snijlijn_2=subs(snijlijn_2,[x,y,z],parparam)

assume([theta,u],'real')
start=snijlijn(2)
stop=snijlijn(1)

normaal=cross(diff(parparam,theta),diff(parparam,u))
norm=norm(normaal)

norm=simplify(norm)
oppervlakte=int(int(norm,u,[start,stop]),theta,[0,2*pi])
%te complex
